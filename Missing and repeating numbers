from os import *
from sys import *
from collections import *
from math import *

def missingAndRepeating(arr, n):
    # Write your code here
    for i in range(n):
        index = abs(arr[i])-1
        if(arr[index]>0):
            arr[index] = -arr[index]
        else:
            repeat = index+1
    for i in range(n):
        if(arr[i]>0):
            return[i+1, repeat]
    pass
