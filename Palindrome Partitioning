from os import *
from sys import *
from collections import *
from math import *

from sys import stdin
import json

def partition(s):
    res=[]
    temp=[]
    func(s,0,temp,res)
    return res
def func(s,index, temp, res):
    if index>=len(s):
        res.append(temp.copy())
        return
    for i in range(index,len(s)):
        if ispalin(s,index,i):
            temp.append(s[index:i+1])
            func(s,i+1,temp,res)
            temp.pop()
def ispalin(s, start, end):
    while(start<end):
        if s[start]!=s[end]:
            return False
        start+=1
        end-=1
    return True

s=stdin.readline().rstrip()

final=partition(s)

for ele in final:
    ele = sorted(ele)
    print(json.dumps(ele))
